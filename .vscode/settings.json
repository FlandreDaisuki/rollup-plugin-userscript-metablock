{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "markdown.extension.toc.levels": "1..3",
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
  },
  "eslint.experimental.useFlatConfig": false,
  "cSpell.words": [
    "antifeature",
    "defaulticon",
    "metablock",
    "metakey",
    "metakeys",
    "nocompat",
    "noframes",
    "rollup"
  ],
  "cSpell.ignorePaths": [
    "dist/*",

    "pnpm-lock.yaml",
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders"
  ]
}
